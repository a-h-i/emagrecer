{
  "compilerOptions": {
    "allowJs": true,
    "outDir": "dist",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "node10",
    "resolveJsonModule": true,
    "incremental": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "rootDir": ".",
    "composite": true,
    "target": "ES2023"
  },
  "exclude": ["node_modules", "dist"],
  "references": [
    {
      "path": "../../packages/storage"
    }
  ]
}
